@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix fed-config: <https://masa.hypotheses.org/federation/config#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://masa.humanum.fr/masa-federation/sparql> a sd:Service ;
    sd:endpoint <http://masa.humanum.fr/masa-federation/sparql> ;
    sd:availableGraphs [
        a sd:GraphCollection ;
        sd:namedGraph [
            a sd:NamedGraph ;
            # FederationSource.getSourceIri
            sd:name <http://masa.humanum.fr/british-museum> ;
            # FederationSource.getLabels
            rdfs:label "Le British Museum"@fr;
            rdfs:label "The British Museum"@en;
            # FederationSource.getEndpoint
            void:sparqlEndpoint <http://collection.britishmuseum.org/sparql> ;
            # optionnel : FederationSource.getDefaultGraph
            # fed-config:sparqlGraph <http://www.exemple.fr/monGraphe> ;
        ] ;
        sd:namedGraph [
            a sd:NamedGraph ;
            # FederationSource.getSourceIri
            sd:name <http://masa.humanum.fr/doremus> ;
            # FederationSource.getLabels
            rdfs:label "Doremus"@fr;
            rdfs:label "Doremus"@en;
            # FederationSource.getEndpoint
            void:sparqlEndpoint <http://data.doremus.org/sparql> ;
            # optionnel : FederationSource.getDefaultGraph
            # fed-config:sparqlGraph <http://www.exemple.fr/monGraphe> ;
        ] ;
        sd:namedGraph [
            a sd:NamedGraph ;
            # FederationSource.getSourceIri
            sd:name <http://masa.humanum.fr/ycbt> ;
            # FederationSource.getLabels
            rdfs:label "Yale Center for British Art"@en;
            # FederationSource.getEndpoint
            void:sparqlEndpoint <http://collection.britishart.yale.edu/sparql/> ;
            # optionnel : FederationSource.getDefaultGraph
            # fed-config:sparqlGraph <http://www.exemple.fr/monGraphe> ;
        ] ;
    ] .
