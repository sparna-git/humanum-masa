[
	{
		"title":"Des personnes dans le British Museum",
		"query": [
				 "PREFIX  skos: <http://www.w3.org/2004/02/skos/core#>",
			 	 "PREFIX  crm:  <http://www.cidoc-crm.org/cidoc-crm/>",
			 	 "SELECT  ?actor ",
			 	 "FROM <http://masa.humanum.fr/british-museum>",
			 	 "WHERE {",
			 	 "  ?actor  a  ?type",
			 	 "  VALUES ?type { crm:E21_Person }",
			 	 "}",
			 	 "LIMIT 20"
		]				
	},
	{
